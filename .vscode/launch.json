    "version": "0.2.0",
    "configurations": [
        {
            "name": "gdb-fishbot-test",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/transforms3d_cpp_test",
            "args": [
                "--gtest_filter=TestTransFormGroup.TransPcl",
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}